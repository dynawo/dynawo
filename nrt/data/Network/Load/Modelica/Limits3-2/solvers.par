<parametersSet xmlns="http://www.rte-france.com/dynawo">
  <set id="SIM">
    <par type="DOUBLE" name="hMin" value="0.0001"/>
    <par type="DOUBLE" name="hMax" value="0.1"/>
    <par type="DOUBLE" name="minimalAcceptableStep" value="0.001"/>
    <par type="DOUBLE" name="fnormtol" value="0.001"/>
    <par type="DOUBLE" name="kReduceStep" value="0.5"/>
    <par type="INT" name="maxNewtonTry" value="10"/>
    <par type="STRING" name="linearSolverName" value="KLU"/>
    <par type="BOOL" name="optimizeAlgebraicResidualsEvaluations" value="false"/>
    <par type="BOOL" name="skipNRIfInitialGuessOK" value="false"/>
    <par type="BOOL" name="enableSilentZ" value="false"/>
    <par type="BOOL" name="optimizeReinitAlgebraicResidualsEvaluations" value="false"/>
    <par type="STRING" name="minimumModeChangeTypeForAlgebraicRestoration" value="ALGEBRAIC"/>
  </set>
</parametersSet>
