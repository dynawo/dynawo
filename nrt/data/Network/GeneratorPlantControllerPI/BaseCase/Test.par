<parametersSet xmlns="http://www.rte-france.com/dynawo">
  <set id="InfiniteBus">
    <reference name="infiniteBus_UPu" origData="IIDM" origName="v_pu" type="DOUBLE"/>
    <reference name="infiniteBus_UPhase" origData="IIDM" origName="angle_pu" type="DOUBLE"/>
  </set>
  <set id="GeneratorPlantControllerPI">
    <reference name="generator_P0Pu" origData="IIDM" origName="p_pu" type="DOUBLE"/>
    <reference name="generator_Q0Pu" origData="IIDM" origName="q_pu" type="DOUBLE"/>
    <reference name="generator_U0Pu" origData="IIDM" origName="v_pu" type="DOUBLE"/>
    <reference name="generator_UPhase0" origData="IIDM" origName="angle_pu" type="DOUBLE"/>
    <reference name="generator_QReg0Pu" origData="IIDM" origName="q_pu" type="DOUBLE"/>
    <reference name="generator_UReg0Pu" origData="IIDM" origName="v_pu" type="DOUBLE"/>
    <par type="DOUBLE" name="generator_SNom" value="15"/>
    <par type="DOUBLE" name="generator_RPu" value="0"/>
    <par type="DOUBLE" name="generator_XPu" value="0"/>
    <par type="DOUBLE" name="generator_LambdaPuSNom" value="0.21"/>
    <par type="DOUBLE" name="generator_AlphaPuPNom" value="0.05"/>
    <par type="DOUBLE" name="generator_QMinReg" value="-10"/>
    <par type="DOUBLE" name="generator_QMaxReg" value="10"/>
    <par type="DOUBLE" name="generator_QMinPu" value="-99"/>
    <par type="DOUBLE" name="generator_QMaxPu" value="99"/>
    <par type="DOUBLE" name="generator_PMin" value="0"/>
    <par type="DOUBLE" name="generator_PMax" value="20"/>
    <par type="DOUBLE" name="generator_PNom" value="15"/>
    <par type="DOUBLE" name="generator_EMaxPu" value="99"/>
    <par type="DOUBLE" name="generator_EMinPu" value="-99"/>
    <par type="DOUBLE" name="generator_Ki" value="10"/>
    <par type="DOUBLE" name="generator_Kp" value="5"/>
    <par type="DOUBLE" name="generator_DbdPu" value="0.05"/>
  </set>
  <set id="OmegaRef">
    <par type="DOUBLE" name="setPoint_Value0" value="1"/>
  </set>
  <set id="LoadPQ">
    <reference name="load_P0Pu" origData="IIDM" origName="p_pu" type="DOUBLE"/>
    <reference name="load_Q0Pu" origData="IIDM" origName="q_pu" type="DOUBLE"/>
    <reference name="load_U0Pu" origData="IIDM" origName="v_pu" type="DOUBLE"/>
    <reference name="load_UPhase0" origData="IIDM" origName="angle_pu" type="DOUBLE"/>
  </set>
  <set id="Bus1">
    <reference type="DOUBLE" name="bus_UNom" origData="IIDM" origName="UNom"/>
    <reference name="bus_U0Pu" origData="IIDM" origName="Upu" type="DOUBLE"/>
    <reference name="bus_UPhase0" origData="IIDM" origName="Theta_pu" type="DOUBLE"/>
  </set>
  <set id="Bus2">
    <reference name="bus_UNom" origData="IIDM" origName="UNom" type="DOUBLE"/>
    <reference name="bus_U0Pu" origData="IIDM" origName="Upu" type="DOUBLE"/>
    <reference name="bus_UPhase0" origData="IIDM" origName="Theta_pu" type="DOUBLE"/>
  </set>
  <set id="Transformer">
    <par type="DOUBLE" name="transformer_R" value="0.001"/>
    <par type="DOUBLE" name="transformer_X" value="0.006"/>
    <par type="DOUBLE" name="transformer_B" value="0.0001"/>
    <par type="DOUBLE" name="transformer_G" value="0.0001"/>
    <reference name="transformer_RatedU1" origData="IIDM" origName="ratedU1" type="DOUBLE"/>
    <reference name="transformer_RatedU2" origData="IIDM" origName="ratedU2" type="DOUBLE"/>
    <reference name="transformer_U1Nom" origData="IIDM" origName="v1Nom" type="DOUBLE"/>
    <reference name="transformer_U2Nom" origData="IIDM" origName="v2Nom" type="DOUBLE"/>
    <reference name="transformer_P10Pu" origData="IIDM" origName="p1_pu" type="DOUBLE"/>
    <reference name="transformer_Q10Pu" origData="IIDM" origName="q1_pu" type="DOUBLE"/>
    <reference name="transformer_U10Pu" origData="IIDM" origName="v1_pu" type="DOUBLE"/>
    <reference name="transformer_U1Phase0" origData="IIDM" origName="angle1_pu" type="DOUBLE"/>
  </set>
</parametersSet>
