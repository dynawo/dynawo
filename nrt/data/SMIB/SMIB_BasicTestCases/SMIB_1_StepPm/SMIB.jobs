<dyn:jobs xmlns:dyn="http://www.rte-france.com/dynawo">
  <dyn:job name="SMIB_StepPm">
      <dyn:solver lib="dynawo_SolverIDA" parFile="solvers.par" parId="5"/>
      <dyn:modeler compileDir="outputs/compilation">
        <dyn:dynModels dydFile="SMIB.dyd"/>
        <dyn:precompiledModels useStandardModels="true"/>
        <dyn:modelicaModels useStandardModels="true">
          <dyn:directory path="." recursive="false"/>
        </dyn:modelicaModels>
      </dyn:modeler>
      <dyn:simulation startTime="0" stopTime="30"/>
      <dyn:outputs directory="outputs">
        <dyn:dumpInitValues local="false" global="true"/>
        <dyn:curves inputFile="curves.crv" exportMode="CSV"/>
        <dyn:logs>
          <dyn:appender tag="" file="dynawo.log" lvlFilter="DEBUG"/>
          <dyn:appender tag="COMPILE" file="dynawoCompiler.log" lvlFilter="DEBUG"/>
        </dyn:logs>
      </dyn:outputs>
  </dyn:job>
</dyn:jobs>
