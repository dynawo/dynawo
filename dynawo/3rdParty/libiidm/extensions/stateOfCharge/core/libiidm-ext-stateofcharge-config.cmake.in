# This file is part of Libiidm, a library to model IIDM networks and allows importing and exporting them to files.

# - Find LibIIDM StateOfCharge extension library and headers
#  The module defines the following variables:
#
# LibIIDM_EXT_STATEOFCHARGE_FOUND        - TRUE
# LibIIDM_EXT_STATEOFCHARGE_VERSION      - Version
# LibIIDM_EXT_STATEOFCHARGE_INCLUDE_DIR  - include search path
# LibIIDM_EXT_STATEOFCHARGE_LIB_DIR      - library path search path
# LibIIDM_EXT_STATEOFCHARGE_BIN_DIR      - binary search path (windows dll)

set(LibIIDM_EXT_STATEOFCHARGE_VERSION @IIDM_VERSION@)

@PACKAGE_INIT@

set_and_check(LibIIDM_EXT_STATEOFCHARGE_INCLUDE_DIR "@PACKAGE_LibIIDM_INSTALL_INCLUDE@")
set_and_check(LibIIDM_EXT_STATEOFCHARGE_LIB_DIR "@PACKAGE_LibIIDM_INSTALL_LIBRARIES@")

if (WIN32)
  set_and_check(LibIIDM_EXT_STATEOFCHARGE_BIN_DIR "@PACKAGE_LibIIDM_INSTALL_BINARIES@")
endif()

include(${CMAKE_CURRENT_LIST_DIR}/libiidm-ext-stateofcharge-targets.cmake)

set(LibIIDM_EXT_STATEOFCHARGE_FOUND TRUE)
mark_as_advanced(LibIIDM_EXT_STATEOFCHARGE_FOUND)
