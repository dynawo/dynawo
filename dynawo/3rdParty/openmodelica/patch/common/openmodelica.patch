+public function getCountBefore
+  input  list<Integer> indexList;
+  output Integer nbCrefPrevious;
+algorithm
+ nbCrefPrevious := matchcontinue(indexList)
+  case({}) then 1;
+  case(_)  then List.last(indexList);
+ end matchcontinue;
+end getCountBefore;
