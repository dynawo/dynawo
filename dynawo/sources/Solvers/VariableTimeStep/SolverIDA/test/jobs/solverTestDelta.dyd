<dyn:dynamicModelsArchitecture xmlns:dyn="http://www.rte-france.com/dynawo">
  <dyn:modelicaModel id="lineDisconnection">
    <dyn:unitDynamicModel id="event" name="DYNDisconnection" moFile="DYNSolverTest.mo"/>
  </dyn:modelicaModel>
  <dyn:modelicaModel id="infiniteBus" staticId="GEN">
    <dyn:unitDynamicModel id="InfiniteBus" name="DYNInfiniteBus" moFile="DYNSolverTest.mo" parFile="jobs/solverTestDelta.par" parId="1"/>
  </dyn:modelicaModel>
  <dyn:modelicaModel id="derivative">
    <dyn:unitDynamicModel id="Derivative" name="DYNDerivative" moFile="DYNSolverTest.mo"/>
  </dyn:modelicaModel>
  <dyn:connect id1="lineDisconnection" var1="event_switchOff" id2="NETWORK" var2="_BUS___1__BUS___2_2_state"/>
  <dyn:connect id1="infiniteBus" var1="InfiniteBus_terminal" id2="NETWORK" var2="TN_ACPIN"/>
  <!-- Adding a connection to a model with a derivative part to test the algebraic mode restoration on differential variable -->
  <dyn:connect id1="derivative" var1="Derivative_x" id2="NETWORK" var2="_BUS___21_Upu"/>
</dyn:dynamicModelsArchitecture>
