<dyn:dynamicModelsArchitecture xmlns:dyn="http://www.rte-france.com/dynawo">
  <!-- Machine with four windings, standard IEEE regulations (GovCt2, St4b) and an auxiliary. -->
  <dyn:modelicaModel id="GeneratorSynchronousFourWindingsGovCt2St4bAux">
    <dyn:unitDynamicModel id="generator" name="Dynawo.Electrical.Machines.OmegaRef.GeneratorSynchronous" initName="Dynawo.Electrical.Machines.OmegaRef.GeneratorSynchronousExtTfo4W_INIT"/>
    <dyn:unitDynamicModel id="voltageRegulator" name="Dynawo.Electrical.Controls.Machines.VoltageRegulators.Standard.St4b" initName="Dynawo.Electrical.Controls.Machines.VoltageRegulators.Standard.St4b_INIT"/>
    <dyn:unitDynamicModel id="governor" name="Dynawo.Electrical.Controls.Machines.Governors.Standard.Generic.GovCt2" initName="Dynawo.Electrical.Controls.Machines.Governors.GovernorPmPGen_INIT"/>
    <dyn:unitDynamicModel id="coupling" name="Dynawo.Electrical.Switches.IdealSwitch2" initName="Dynawo.Electrical.Switches.IdealSwitchGeneratorAux_INIT"/>
    <dyn:unitDynamicModel id="auxLV" name="Dynawo.Electrical.Loads.LoadAlphaBeta" initName="Dynawo.Electrical.Loads.LoadAuxiliaries_INIT"/>
    <dyn:initConnect id1="voltageRegulator" var1="Us0Pu" id2="generator" var2="UStator0Pu"/>
    <dyn:initConnect id1="voltageRegulator" var1="Efd0Pu" id2="generator" var2="Efd0Pu"/>
    <dyn:initConnect id1="voltageRegulator" var1="ut0Pu" id2="generator" var2="u0Pu"/>
    <dyn:initConnect id1="voltageRegulator" var1="it0Pu" id2="generator" var2="i0Pu"/>
    <dyn:initConnect id1="voltageRegulator" var1="Ir0Pu" id2="generator" var2="IRotor0Pu"/>
    <dyn:initConnect id1="governor" var1="Pm0Pu" id2="generator" var2="Pm0Pu"/>
    <dyn:initConnect id1="governor" var1="PGen0Pu" id2="generator" var2="PGen0Pu"/>
    <dyn:initConnect id1="auxLV" var1="u0Pu" id2="coupling" var2="u20Pu"/>
    <dyn:initConnect id1="generator" var1="P0Pu" id2="coupling" var2="P20Pu"/>
    <dyn:initConnect id1="generator" var1="Q0Pu" id2="coupling" var2="Q20Pu"/>
    <dyn:initConnect id1="generator" var1="U0Pu" id2="coupling" var2="U20Pu"/>
    <dyn:initConnect id1="generator" var1="UPhase0" id2="coupling" var2="U2Phase0"/>
    <dyn:initConnect id1="coupling" var1="PAuxLV0Pu" id2="auxLV" var2="P0Pu"/>
    <dyn:initConnect id1="coupling" var1="QAuxLV0Pu" id2="auxLV" var2="Q0Pu"/>
    <dyn:connect id1="generator" var1="omegaPu" id2="governor" var2="omegaPu"/>
    <dyn:connect id1="generator" var1="PmPu" id2="governor" var2="PmPu"/>
    <dyn:connect id1="generator" var1="UStatorPu" id2="voltageRegulator" var2="UsPu"/>
    <dyn:connect id1="generator" var1="efdPu" id2="voltageRegulator" var2="EfdPu"/>
    <dyn:connect id1="voltageRegulator" var1="running" id2="generator" var2="running.value"/>
    <dyn:connect id1="voltageRegulator" var1="utPu" id2="generator" var2="uPu"/>
    <dyn:connect id1="voltageRegulator" var1="itPu" id2="generator" var2="iStatorPu"/>
    <dyn:connect id1="voltageRegulator" var1="IrPu" id2="generator" var2="IRotorPu"/>
    <dyn:connect id1="governor" var1="PGenPu" id2="generator" var2="PGenPu"/>
    <dyn:connect id1="generator" var1="terminal" id2="coupling" var2="terminal2"/>
    <dyn:connect id1="auxLV" var1="terminal" id2="coupling" var2="terminal2"/>
    <dyn:connect id1="generator" var1="switchOffSignal1" id2="auxLV" var2="switchOffSignal1"/>
    <dyn:connect id1="generator" var1="switchOffSignal1" id2="coupling" var2="switchOffSignal1"/>
  </dyn:modelicaModel>
</dyn:dynamicModelsArchitecture>
