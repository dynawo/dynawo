<dyn:dynamicModelsArchitecture xmlns:dyn="http://www.rte-france.com/dynawo">
  <!-- Machine with four windings, a proportional governor on mechanical power and a proportional integral excitation voltage regulator-->

 <dyn:modelicaModel id="GeneratorSynchronousFourWindingsGoverPropVRPropInt">
    <dyn:unitDynamicModel id="generator" name="Dynawo.Electrical.Machines.GeneratorSynchronous" initName ="Dynawo.Electrical.Machines.GeneratorSynchronousExt_4E_INIT"/>
    <dyn:unitDynamicModel id="voltageRegulator" name="Dynawo.Electrical.Controls.Machines.VoltageRegulators.VRProportionalIntegral" initName="Dynawo.Electrical.Controls.Machines.VoltageRegulators.VRProportionalIntegral_INIT"/>
    <dyn:unitDynamicModel id="governor" name="Dynawo.Electrical.Controls.Machines.Governors.GoverProportional" initName="Dynawo.Electrical.Controls.Machines.Governors.Governor_INIT"/>
    <dyn:unitDynamicModel id="URef" name="Dynawo.Electrical.Controls.Basics.SetPoint" initName="Dynawo.Electrical.Controls.Basics.SetPoint_INIT"/>
    <dyn:unitDynamicModel id="Pm" name="Dynawo.Electrical.Controls.Basics.SetPoint" initName="Dynawo.Electrical.Controls.Basics.SetPoint_INIT"/>
    <dyn:initConnect id1="voltageRegulator" var1="UsRef0Pu" id2="URef" var2="Value0"/>
    <dyn:initConnect id1="voltageRegulator" var1="Efd0PuLF" id2="generator" var2="Efd0Pu"/>
    <dyn:initConnect id1="voltageRegulator" var1="Us0Pu" id2="generator" var2="UStator0Pu"/>
    <dyn:initConnect id1="governor" var1="Pm0Pu" id2="Pm" var2="Value0"/>
    <dyn:initConnect id1="governor" var1="Pm0Pu" id2="generator" var2="Pm0Pu"/>
    <dyn:connect id1="voltageRegulator" var1="UsRefPu" id2="URef" var2="setPoint.value"/>
    <dyn:connect id1="voltageRegulator" var1="EfdPuPin" id2="generator" var2="efdPu"/>
    <dyn:connect id1="voltageRegulator" var1="UsPu" id2="generator" var2="UStatorPu.value"/>
    <dyn:connect id1="governor" var1="PmRefPu" id2="Pm" var2="setPoint"/>
    <dyn:connect id1="governor" var1="omegaPu" id2="generator" var2="omegaPu"/>
    <dyn:connect id1="governor" var1="PmPu" id2="generator" var2="PmPu"/>
  </dyn:modelicaModel>
</dyn:dynamicModelsArchitecture>
