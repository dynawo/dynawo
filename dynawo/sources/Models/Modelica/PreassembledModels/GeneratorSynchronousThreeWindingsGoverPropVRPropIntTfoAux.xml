<dyn:dynamicModelsArchitecture xmlns:dyn="http://www.rte-france.com/dynawo">
  <!-- Machine with three windings, a proportional governor on mechanical power, a proportional integral excitation voltage regulator, a transformer and auxiliaries. -->
  <dyn:modelicaModel id="GeneratorSynchronousThreeWindingsGoverPropVRPropIntTfoAux">
    <dyn:unitDynamicModel id="generator" name="Dynawo.Electrical.Machines.OmegaRef.GeneratorSynchronous" initName ="Dynawo.Electrical.Machines.OmegaRef.GeneratorSynchronousExtTfo3W_INIT"/>
    <dyn:unitDynamicModel id="voltageRegulator" name="Dynawo.Electrical.Controls.Machines.VoltageRegulators.Simplified.VRProportionalIntegral" initName="Dynawo.Electrical.Controls.Machines.VoltageRegulators.Simplified.VRProportionalIntegral_INIT"/>
    <dyn:unitDynamicModel id="governor" name="Dynawo.Electrical.Controls.Machines.Governors.Simplified.GoverProportional" initName="Dynawo.Electrical.Controls.Machines.Governors.Governor_INIT"/>
    <dyn:unitDynamicModel id="transformer" name="Dynawo.Electrical.Transformers.TransformersFixedTap.GeneratorTransformer" initName="Dynawo.Electrical.Transformers.TransformersFixedTap.GeneratorTransformerAux_INIT"/>
    <dyn:unitDynamicModel id="coupling" name="Dynawo.Electrical.Switches.IdealSwitch"/>
    <dyn:unitDynamicModel id="auxLV" name="Dynawo.Electrical.Loads.LoadAlphaBeta" initName="Dynawo.Electrical.Loads.LoadAuxiliaries_INIT"/>
    <dyn:unitDynamicModel id="auxHV" name="Dynawo.Electrical.Loads.LoadAlphaBeta" initName="Dynawo.Electrical.Loads.LoadAuxiliaries_INIT"/>
    <dyn:initConnect id1="voltageRegulator" var1="Efd0PuLF" id2="generator" var2="Efd0Pu"/>
    <dyn:initConnect id1="voltageRegulator" var1="Us0Pu" id2="generator" var2="UStator0Pu"/>
    <dyn:initConnect id1="governor" var1="Pm0Pu" id2="generator" var2="Pm0Pu"/>
    <dyn:initConnect id1="generator" var1="P0Pu" id2="transformer" var2="P20Pu"/>
    <dyn:initConnect id1="generator" var1="Q0Pu" id2="transformer" var2="Q20Pu"/>
    <dyn:initConnect id1="generator" var1="U0Pu" id2="transformer" var2="U20Pu"/>
    <dyn:initConnect id1="generator" var1="UPhase0" id2="transformer" var2="U2Phase0"/>
    <dyn:initConnect id1="auxLV" var1="u0Pu" id2="transformer" var2="u20Pu"/>
    <dyn:initConnect id1="auxHV" var1="u0Pu" id2="transformer" var2="u10Pu"/>
    <dyn:initConnect id1="transformer" var1="PAuxLV0Pu" id2="auxLV" var2="P0Pu"/>
    <dyn:initConnect id1="transformer" var1="QAuxLV0Pu" id2="auxLV" var2="Q0Pu"/>
    <dyn:initConnect id1="transformer" var1="PAuxHV0Pu" id2="auxHV" var2="P0Pu"/>
    <dyn:initConnect id1="transformer" var1="QAuxHV0Pu" id2="auxHV" var2="Q0Pu"/>
    <dyn:connect id1="voltageRegulator" var1="EfdPu" id2="generator" var2="efdPu"/>
    <dyn:connect id1="voltageRegulator" var1="UsPu" id2="generator" var2="UStatorPu"/>
    <dyn:connect id1="governor" var1="omegaPu" id2="generator" var2="omegaPu"/>
    <dyn:connect id1="governor" var1="PmPu" id2="generator" var2="PmPu"/>
    <dyn:connect id1="generator" var1="terminal" id2="transformer" var2="terminal2"/>
    <dyn:connect id1="generator" var1="switchOffSignal1" id2="transformer" var2="switchOffSignal1"/>
    <dyn:connect id1="auxLV" var1="terminal" id2="generator" var2="terminal"/>
    <dyn:connect id1="transformer" var1="terminal1" id2="coupling" var2="terminal2"/>
    <dyn:connect id1="auxHV" var1="terminal" id2="coupling" var2="terminal2"/>
    <dyn:connect id1="generator" var1="switchOffSignal1" id2="auxLV" var2="switchOffSignal1"/>
    <dyn:connect id1="transformer" var1="switchOffSignal1" id2="auxHV" var2="switchOffSignal1"/>
  </dyn:modelicaModel>
</dyn:dynamicModelsArchitecture>
