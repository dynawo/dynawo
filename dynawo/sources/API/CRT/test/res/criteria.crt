<criteria xmlns="http://www.rte-france.com/dynawo">
  <busCriteria>
    <parameters id ="busCritId" scope="DYNAMIC" type="LOCAL_VALUE" uMaxPu="0.8" uNomMin="225"/>
    <component id="MyId"/>
    <component id="MyId2"/>
  </busCriteria>
  <loadCriteria>
    <parameters id ="loadCritId"  scope="FINAL" type="SUM" pMax="200"/>
  </loadCriteria>
  <loadCriteria>
    <parameters id ="loadCritId2"  scope="FINAL" type="SUM" pMax="1500" uMinPu="0.2"/>
    <component id="MyLoad"/>
    <component id="MyLoad2"/>
  </loadCriteria>
</criteria>
