<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:dyn="http://www.rte-france.com/dynawo"
           targetNamespace="http://www.rte-france.com/dynawo"
           elementFormDefault="qualified">
  <xs:element name="finalStateValuesInput">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="finalStateValue" type="dyn:FinalStateValueInput" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
    <xs:unique name="uniqueFinalStateValue">
      <xs:selector xpath="dyn:finalStateValue"/>
      <xs:field xpath="@model"/>
      <xs:field xpath="@variable"/>
    </xs:unique>
  </xs:element>

  <xs:complexType name="FinalStateValueInput">
    <xs:attribute name="model" use="required" type="xs:string"/>
    <xs:attribute name="variable" use="required" type="xs:string"/>
  </xs:complexType>
</xs:schema>
