    <!-- <par type="DOUBLE" name="GFL_U0Pu" value="1.0847"/> -->
    <!-- <par type="DOUBLE" name="GFL_UPhase0" value="-0.18"/> -->
    <par type="DOUBLE" name="GFL_tPFilt" value="0.01"/>
    <par type="DOUBLE" name="GFL_tQFilt" value="0.01"/>
    <par type="DOUBLE" name="GFL_tVSC" value="0.0004"/>
  </set>
  <set id="IDA">
    <par type="INT" name="order" value="2"/>
