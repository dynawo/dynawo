<parametersSet xmlns="http://www.rte-france.com/dynawo">
  <!-- InfiniteBus -->
  <set id="InfiniteBus">
    <par type="DOUBLE" name="infiniteBus_UPu" value="0.90081"/>
    <par type="DOUBLE" name="infiniteBus_UPhase" value="0.0"/>
  </set>
  <!-- line 1 -->
  <set id="Line1">
    <par type="DOUBLE" name="line_RPu" value="0"/>
    <par type="DOUBLE" name="line_XPu" value="0.022522"/>
    <par type="DOUBLE" name="line_BPu" value="0.0"/>
    <par type="DOUBLE" name="line_GPu" value="0.0"/>
  </set>
  <!-- line 2 -->
  <set id="Line2">
    <par type="DOUBLE" name="line_RPu" value="0"/>
    <par type="DOUBLE" name="line_XPu" value="0.04189"/>
    <par type="DOUBLE" name="line_BPu" value="0"/>
    <par type="DOUBLE" name="line_GPu" value="0"/>
  </set>
  <!-- tfo -->
  <set id="Transformer">
    <par type="DOUBLE" name="transformer_RPu" value="0"/>
    <par type="DOUBLE" name="transformer_XPu" value="0.00675"/>
    <par type="DOUBLE" name="transformer_BPu" value="0.0"/>
    <par type="DOUBLE" name="transformer_GPu" value="0.0"/>
    <par type="DOUBLE" name="transformer_rTfoPu" value="1"/>
  </set>
  <set id="SynchronousMachine">
    <!-- Synchronous machine external parameters -->
    <par type="DOUBLE" name="generator_md" value="0"/>
    <par type="DOUBLE" name="generator_mq" value="0"/>
    <par type="DOUBLE" name="generator_nd" value="0"/>
    <par type="DOUBLE" name="generator_nq" value="0"/>
    <par type="INT" name="generator_ExcitationPu" value="1"/>
    <par type="BOOL" name="generator_UseApproximation" value="true"/>
    <par type="DOUBLE" name="generator_MdPuEfd" value="0"/>
    <par type="DOUBLE" name="generator_H" value="3.5"/>
    <par type="DOUBLE" name="generator_DPu" value="0.0"/>
    <par type="DOUBLE" name="generator_RaPu" value="0.003"/>
    <par type="DOUBLE" name="generator_XlPu" value="0.15"/>
    <par type="DOUBLE" name="generator_XdPu" value="1.81"/>
    <par type="DOUBLE" name="generator_XpdPu" value="0.30"/>
    <par type="DOUBLE" name="generator_XppdPu" value="0.23"/>
    <par type="DOUBLE" name="generator_Tpd0" value="8"/>
    <par type="DOUBLE" name="generator_Tppd0" value="0.03"/>
    <par type="DOUBLE" name="generator_XqPu" value="1.76"/>
    <par type="DOUBLE" name="generator_XpqPu" value="0.65"/>
    <par type="DOUBLE" name="generator_XppqPu" value="0.25"/>
    <par type="DOUBLE" name="generator_Tpq0" value="1"/>
    <par type="DOUBLE" name="generator_Tppq0" value="0.07"/>
    <par type="DOUBLE" name="generator_UNom" value="24"/>
    <par type="DOUBLE" name="generator_SNom" value="2220"/>
    <par type="DOUBLE" name="generator_PNomTurb" value="2220"/>
    <par type="DOUBLE" name="generator_PNomAlt" value="2200"/>
    <par type="DOUBLE" name="generator_SnTfo" value="2220"/>
    <par type="DOUBLE" name="generator_UNomHV" value="400"/>
    <par type="DOUBLE" name="generator_UNomLV" value="24"/>
    <par type="DOUBLE" name="generator_UBaseHV" value="400"/>
    <par type="DOUBLE" name="generator_UBaseLV" value="24"/>
    <par type="DOUBLE" name="generator_RTfPu" value="0"/>
    <par type="DOUBLE" name="generator_XTfPu" value="0"/>
    <par type="DOUBLE" name="generator_P0Pu" value="-19.98"/>
    <par type="DOUBLE" name="generator_Q0Pu" value="-9.68"/>
    <par type="DOUBLE" name="generator_U0Pu" value="1"/>
    <par type="DOUBLE" name="generator_UPhase0" value="0.49445"/>
    <!-- AVR -->
    <par type="DOUBLE" name="voltageRegulator_EfdMaxPu" value="7"/>
    <par type="DOUBLE" name="voltageRegulator_EfdMinPu" value="-6.4"/>
    <par type="DOUBLE" name="voltageRegulator_Ka" value="200"/>
    <par type="DOUBLE" name="voltageRegulator_tR" value="0.015"/>
    <!-- PSS -->
    <par type="DOUBLE" name="pss_KStab" value="9.5"/>
    <par type="DOUBLE" name="pss_t1" value="0.154"/>
    <par type="DOUBLE" name="pss_t2" value="0.033"/>
    <par type="DOUBLE" name="pss_tW" value="1.41"/>
    <par type="DOUBLE" name="pss_VsMinPu" value="-0.2"/>
    <par type="DOUBLE" name="pss_VsMaxPu" value="0.2"/>
  </set>
  <!-- OmegaRef -->
  <set id="SetPointOmegaRef">
    <par type="DOUBLE" name="setPoint_Value0" value="1"/>
  </set>
  <!-- Fault -->
  <set id="Fault">
    <par type="DOUBLE" name="fault_tBegin" value="1"/>
    <par type="DOUBLE" name="fault_tEnd" value="1.07"/>
    <par type="DOUBLE" name="fault_RPu" value="0"/>
    <par type="DOUBLE" name="fault_XPu" value="0"/>
  </set>
  <!-- Line opening -->
  <set id="LineOpening">
    <par type="DOUBLE" name="event_tEvent" value="1.07"/>
    <par type="BOOL" name="event_stateEvent1" value="true"/>
  </set>
  <!-- IDA order 2 solver -->
  <set id="IDAOrder2">
    <par type="INT" name="order" value="2"/>
    <par type="DOUBLE" name="initStep" value="0.000001"/>
    <par type="DOUBLE" name="minStep" value="0.000001"/>
    <par type="DOUBLE" name="maxStep" value="10"/>
    <par type="DOUBLE" name="absAccuracy" value="1e-4"/>
    <par type="DOUBLE" name="relAccuracy" value="1e-4"/>
  </set>
</parametersSet>
