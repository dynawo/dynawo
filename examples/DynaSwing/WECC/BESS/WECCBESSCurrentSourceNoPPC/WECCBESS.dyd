  <dyn:blackBoxModel id="PFaRef" lib="Step" parFile="WECCBESS.par" parId="PFaRef"/>
  <dyn:blackBoxModel id="Line" lib="Line" parFile="WECCBESS.par" parId="LinePar"/>

  <dyn:connect id1="BESS" var1="BESS_terminal" id2="Line" var2="line_terminal1"/>
  <dyn:connect id1="Line" var1="line_terminal2" id2="InfBus" var2="infiniteBus_terminal"/>
