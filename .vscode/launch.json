{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch Dynawo",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build-code/install/dynawo/bin/dynawo",
            "args": [ "${workspaceFolder}/nrt/data/IEEE14/IEEE14_SyntaxExamples/IEEE14_BlackBoxModels/IEEE14.jobs" ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                { "name": "DYNAWO_DDB_DIR", "value" : "${workspaceFolder}/build-code/install/dynawo/ddb" },
                { "name": "DYNAWO_DICTIONARIES", "value" : "dictionaries_mapping" },
                { "name": "DYNAWO_INSTALL_DIR", "value" : "${workspaceFolder}/build-code/install/dynawo" },
                { "name": "DYNAWO_LOCALE", "value" : "en_GB" },
                { "name": "DYNAWO_RESOURCES_DIR", "value" : "${workspaceFolder}/build-code/install/dynawo/share/" },
                { "name": "DYNAWO_SCRIPTS_DIR", "value" : "${workspaceFolder}/build-code/install/dynawo/sbin" },
                { "name": "DYNAWO_USE_XSD_VALIDATION", "value" : "true" },
                { "name": "DYNAWO_XSD_DIR", "value" : "${workspaceFolder}/build-code/install/dynawo/share/xsd/" },
                { "name": "IIDM_XML_XSD_PATH", "value" : "${workspaceFolder}/build-code/install/3rdParty/libiidm/share/iidm/xsd/" },
                { "name": "DYNAWO_ADEPT_INSTALL_DIR", "value" : "${workspaceFolder}/build-code/install/3rdParty/adept" },
                { "name": "DYNAWO_NICSLU_INSTALL_DIR", "value" : "${workspaceFolder}/build-code/install/3rdParty/nicslu" },
                { "name": "DYNAWO_SUITESPARSE_INSTALL_DIR", "value" : "${workspaceFolder}/build-code/install/3rdParty/suitesparse" },
                { "name": "DYNAWO_SUNDIALS_INSTALL_DIR", "value" : "${workspaceFolder}/build-code/install/3rdParty/sundials" },
                { "name": "DYNAWO_XERCESC_INSTALL_DIR", "value" : "${workspaceFolder}/build-code/install/3rdParty/xerces-c" },
                { "name": "DYNAWO_LIBXML_HOME", "value" : "${workspaceFolder}/build-code/install/3rdParty/libxml" },
                { "name": "DYNAWO_INSTALL_OPENMODELICA", "value" : "${workspaceFolder}/OpenModelica/install" },
                { "name": "DYNAWO_OPENMODELICA_VERSION", "value" : "1_13_2" },
                { "name": "DYNAWO_LIBIIDM_EXTENSIONS", "value" : "${workspaceFolder}/build-code/install/3rdParty/libiidm/lib" },
                { "name": "LD_LIBRARY_PATH", "value" : "${env:LD_LIBRARY_PATH}:${workspaceFolder}/build-code/install/dynawo/lib:${workspaceFolder}/build-code/install/3rdParty/adept/lib:${workspaceFolder}/build-code/install/3rdParty/libiidm/lib:${workspaceFolder}/build-code/install/3rdParty/libxml/lib:${workspaceFolder}/build-code/install/3rdParty/libzip/lib:${workspaceFolder}/build-code/install/3rdParty/nicslu/lib:${workspaceFolder}/build-code/install/3rdParty/suitesparse/lib:${workspaceFolder}/build-code/install/3rdParty/sundials/lib:${workspaceFolder}/build-code/install/3rdParty/xerces-c/lib" }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
